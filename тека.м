модуль М
  ПІ число = global.Math.PI ;;число пі, приблизно 3.14
  Е число = global.Math.E ;;число е, приблизно 2.718
  ЛОГ10 число = global.Math.LN10 ;;натуральний логарифм 10, приблизно 2.3
  ЛОГ2 число = global.Math.LN2 ;;натуральний логарифм 2, приблизно 0.69
  ЛОГ10Е число = global.Math.LOG10E ;;логарифм числа е по базі 10, приблизно 0.43
  ЛОГ2Е число = global.Math.LOG2E ;;логарифм числа е по базі 2, приблизно 1.44
  КОРІНЬ1_2 число = global.Math.SQRT1_2 ;;квадратний корінь 1/2, приблизно 0.7
  КОРІНЬ2 число = global.Math.SQRT2 ;;квадратний корінь 2, приблизно 1.414

  дія корінь(а число) число ;;повернути квадратний корінь числа
    global.Math.sqrt(а)
  кінець

  дія синус(а число) число ;;повернути синус числа
    global.Math.sin(а)
  кінець

  дія косинус(а число) число ;;повернути косинус числа
    global.Math.cos(а)
  кінець

  дія гіпотенуза(а число, б число) число ;;повернути гіпотенузу прямокутного трикутника з даними катетами
    global.Math.hypot(а, б)
  кінець

  дія логарифм(а число) число ;;повернути натуральний логарифм (база е) числа
    global.Math.log(а)
  кінець

  дія логарифм10(а число) число ;;повернути логарифм по базі 10 числа
    global.Math.log10(а)
  кінець

  дія логарифм2(а число) число ;;повернути логарифм по базі 2 числа
    global.Math.log2(а)
  кінець

  дія округлити(а число) число ;;округлити число до найближчого цілого значення
    global.Math.round(а)
  кінець

  дати ПІ
  дати Е
  дати ЛОГ10
  дати ЛОГ2
  дати ЛОГ10Е
  дати ЛОГ2Е
  дати КОРІНЬ1_2
  дати КОРІНЬ2
  дати корінь
  дати синус
  дати косинус
  дати гіпотенуза
  дати логарифм
  дати логарифм10
  дати логарифм2
  дати округлити
кінець
